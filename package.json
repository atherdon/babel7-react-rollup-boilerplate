{
  "name": "babel7-react-rollup-boilerplate",
  "version": "0.0.1",
  "description": "Clean template for React projects with Babel and Rollup",
  "main": "index.js",
  "repository": "https://github.com/hirdbluebird/babel7-react-rollup-boilerplate.git",
  "author": "hirdbluebird",
  "license": "MIT",
  "scripts": {
    "bundle": "rollup -c",
    "build": "babel src -d dist",
    "build:test": "shx rm -rf && babel src -d dist",
    "#prepare": "yarn clean",
    "clean": "shx rm -rf ./coverage && shx rm -rf ./dist",
    "lint": "standard",
    "zprepublish": "yarn clean && yarn build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cover": "jest --coverage",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "code-fix": "standard --fix"
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "rollup": "^1.14.4",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.0.1",
    "rollup-plugin-replace": "^2.2.0"
  }
}
